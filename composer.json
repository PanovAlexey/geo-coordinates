{
    "name": "codeblog.pro/geo-coordinates",
    "type": "library",
    "description": "The package provides an entity of geographic coordinates.",
    "keywords": [
        "codeblog.pro",
        "geo-coordinates",
        "geo",
        "coordinates",
        "geographical-coordinates"
    ],
    "homepage": "https://github.com/PanovAlexey/geo-coordinates",
    "license": "MIT",
    "authors": [
        {
            "name": "Panov Alexey",
            "email": "panov@codeblog.pro",
            "homepage": "https://codeblog.pro/",
            "role": "Owner"
        }
    ],
    "require": {
        "php" : ">=7.4"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.0.0",
        "phpstan/phpstan": "^0.12.36",
        "nunomaduro/phpinsights": "^1.14",
        "phpunit/php-code-coverage": "^8.0",
        "squizlabs/php_codesniffer": "^3.0"
    },
    "autoload": {
        "psr-4": {
            "CodeblogPro\\GeoCoordinates\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "CodeblogPro\\GeoCoder\\GeoCoordinates\\": "tests/"
        }
    },
    "scripts": {
        "test": "phpunit",
        "check-style": "phpcs src tests",
        "fix-style": "phpcbf src tests"
    },
    "config": {
        "sort-packages": true
    }
}
